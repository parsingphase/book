{
    "name": "parsingphase/book",
    "description": "Book module for Adze applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard George",
            "email": "richard@phase.org"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/parsingphase/adze.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Phase\\Book\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~5.5",
        "doctrine/dbal": "~2.4",
        "parsingphase/adze": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "theseer/phpdox": "~0.6",
        "phploc/phploc": "~2.0",
        "squizlabs/php_codesniffer": "~1.5",
        "ext-pdo_sqlite": "*",
        "leafo/lessphp": "~0.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
